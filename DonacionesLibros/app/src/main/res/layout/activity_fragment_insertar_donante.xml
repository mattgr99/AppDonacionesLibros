<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.fragmentInsertarDonante">

    <TextView
        android:id="@+id/textView"
        android:layout_width="95dp"
        android:layout_height="37dp"
        android:layout_x="128dp"
        android:layout_y="12dp"
        android:text="Donante" />

    <Button
        android:id="@+id/bt_listar_donantes"
        android:layout_width="103dp"
        android:layout_height="110dp"
        android:layout_x="164dp"
        android:layout_y="379dp"
        android:background="@drawable/ic_list" />

    <Button
        android:id="@+id/bt_registrar_donante"
        android:layout_width="wrap_content"
        android:layout_height="77dp"
        android:layout_x="34dp"
        android:layout_y="390dp"
        android:background="@drawable/ic_save" />

    <TextView
        android:id="@+id/textView43"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="163dp"
        android:layout_y="363dp"
        android:text="Listado Donantes" />

    <EditText
        android:id="@+id/et_direccion_donante"
        android:layout_width="162dp"
        android:layout_height="wrap_content"
        android:layout_x="145dp"
        android:layout_y="305dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine" />

    <EditText
        android:id="@+id/et__telefono_donante"
        android:layout_width="178dp"
        android:layout_height="wrap_content"
        android:layout_x="144dp"
        android:layout_y="221dp"
        android:ems="10"
        android:inputType="number" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="69dp"
        android:layout_height="wrap_content"
        android:layout_x="23dp"
        android:layout_y="236dp"
        android:text="Teléfono" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="68dp"
        android:layout_height="wrap_content"
        android:layout_x="22dp"
        android:layout_y="174dp"
        android:text="Nombre" />

    <EditText
        android:id="@+id/et_nombre_donante"
        android:layout_width="176dp"
        android:layout_height="wrap_content"
        android:layout_x="141dp"
        android:layout_y="149dp"
        android:ems="10"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/et_cedula_donante"
        android:layout_width="178dp"
        android:layout_height="wrap_content"
        android:layout_x="139dp"
        android:layout_y="77dp"
        android:ems="10"
        android:inputType="number" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="71dp"
        android:layout_height="36dp"
        android:layout_x="17dp"
        android:layout_y="90dp"
        android:text="Cédula" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="71dp"
        android:layout_height="wrap_content"
        android:layout_x="27dp"
        android:layout_y="316dp"
        android:text="Direccion" />

</AbsoluteLayout>